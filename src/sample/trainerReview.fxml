<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.trainerReviewController">
   <children>
      <AnchorPane prefHeight="82.0" prefWidth="520.0" style="-fx-background-color: #333;">
         <children>
            <Text fill="WHITE" layoutX="178.0" layoutY="46.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Trainer Review">
               <font>
                  <Font name="Baloo Da Regular" size="24.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
      <TextArea fx:id="reviewTextArea" layoutX="60.0" layoutY="111.0" prefHeight="230.0" prefWidth="400.0" promptText="Insert your text here" />
      <Button fx:id="submitButton" layoutX="143.0" layoutY="376.0" mnemonicParsing="false" onAction="#submitButtonOnAction" prefHeight="40.0" prefWidth="235.0" style="-fx-background-color: #333;" text="Submit" textFill="WHITE" />
      <Button fx:id="cancelButton" layoutX="143.0" layoutY="434.0" mnemonicParsing="false" onAction="#cancelButtonOnAction" prefHeight="40.0" prefWidth="235.0" style="-fx-background-color: #333;" text="Cancel" textFill="WHITE" />
      <Label fx:id="successLabel" alignment="CENTER" layoutX="105.0" layoutY="350.0" prefHeight="17.0" prefWidth="312.0" textFill="#1b59cd" />
   </children>
</AnchorPane>
